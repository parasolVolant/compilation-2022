	mov	dword [tab+r26],	4	;Affect
	mov	r27,	5	;
	imul	r27,	4	;
	mov	dword [tab+r27],	2	;Affect
	mov	r28,	6	;
	imul	r28,	4	;
	mov	dword [tab+r28],	3	;Affect
